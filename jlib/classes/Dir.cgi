package Dir;
$name = 'Директория';
$page = '/page.ehtml';
$pages_direction = 0;
#@aview = qw/name onpage/;
$add  = 'Elem Dir';
@ISA = 'DBArray';
use strict qw(subs vars);

sub props
{
	my %props = (

		'name'	  => { 'type' => 'string', 'length' => 100, 'name' => 'Название' },
		'onpage'  => { 'type' => 'int', 'name' => 'Элементов на странице' }
	);

	return %props;
}

sub new
{
	my $o = {};
	bless($o);

	$o->_construct(@_);

	return $o;
}

sub DESTROY
{
	my $o = shift;
	$o->SUPER::DESTROY();
}

return 1;